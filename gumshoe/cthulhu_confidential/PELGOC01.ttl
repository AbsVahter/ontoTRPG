                       bc:next :during_the_game ;
                       rdfs:label "Beginning of the game" .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/gumshoe/cthulhu_confidential/PELGOC01.ttl#biology
:biology rdf:type owl:NamedIndividual ,
                  :InvestigativeAbilityAcademic ;
         sche:description "You study the evolution, behavior, and physical makeup of living organisms." ;
         rdfs:label "Biology investigative ability" .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/gumshoe/cthulhu_confidential/PELGOC01.ttl#bureaucracy
:bureaucracy rdf:type owl:NamedIndividual ,
                      :InvestigativeAbilityInterpersonal ;
             sche:description "You expertly navigate bureaucratic organization, from government offices to large private firms. From the hidden reasons behind a bureaucracy’s opaque decisions to the arcane details of its paperwork, you can extract the information you seek smoothly and with a minimum of ruffled feathers." ;
             rdfs:label "Bureaucracy investigative ability" .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/gumshoe/cthulhu_confidential/PELGOC01.ttl#check_outcome_advance
:check_outcome_advance rdf:type owl:NamedIndividual ;
                       bc:else :check_outcome_hold ;
                       bc:next :gain_push ;
                       bc:set :edges ;
                       bc:requirement "dice roll >= playing_case.scene.test.thresholdAdvance" ;
                       bc:value "outcome = narrative, possible edge" ;
                       sche:description "succeeding especially well" ;
                       rdfs:label "Check the dice roll against the advance threshold" .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/gumshoe/cthulhu_confidential/PELGOC01.ttl#check_outcome_hold
:check_outcome_hold rdf:type owl:NamedIndividual ;
                    bc:else :get_outcome_setback ;
                    bc:requirement "(playing_case.scene.test.thresholdHold exists) and (dice roll >= playing_case.scene.test.thresholdHold)" ;
                    bc:value "outcome = narrative" ;
                    sche:description "generally leaves you no worse off than you were before, or allows you to move forward, though without additional benefit" ;
                    rdfs:label "Check the dice roll against the hold threshold" .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/gumshoe/cthulhu_confidential/PELGOC01.ttl#chemistry
:chemistry rdf:type owl:NamedIndividual ,
                    :InvestigativeAbilityTechnical ;
           sche:description "You’re trained in the analysis of chemical substances." ;
           rdfs:label "Chemistry investigative ability" .


