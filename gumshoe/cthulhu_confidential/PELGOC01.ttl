###  https://raw.githubusercontent.com/AbsVahter/trpgontologies/main/gumshoe/cthulhu_confidential/PELGOC01.ttl#advance
:advance rdf:type owl:ObjectProperty ;
         rdfs:domain :Challenge ;
         rdfs:range :Gamerule ;
         rdfs:comment "Succeeding especially well. This might grant you an Edge card, or allow you extra leverage in the situation at hand." ;
         rdfs:label "Advance outcome" ;
         rdfs:seeAlso pzo2101:checkCriticalSuccess .


###  https://raw.githubusercontent.com/AbsVahter/trpgontologies/main/gumshoe/cthulhu_confidential/PELGOC01.ttl#hold
:hold rdf:type owl:ObjectProperty ;
      rdfs:domain :Challenge ;
      rdfs:range :Gamerule ;
      rdfs:comment "generally leaves you no worse off than you were before, or allows you to move forward, though without additional benefit" ;
      rdfs:label "Hold outcome" ;
      rdfs:seeAlso pzo2101:checkFailure ,
                   pzo2101:checkSuccess .


###  https://raw.githubusercontent.com/AbsVahter/trpgontologies/main/gumshoe/cthulhu_confidential/PELGOC01.ttl#setback
:setback rdf:type owl:ObjectProperty ;
         rdfs:domain :Challenge ;
         rdfs:range :Gamerule ;
         rdfs:comment "Worsening your situation. It may saddle you with a Problem card. Or it might simply make your immediate dilemma worse in some way." ;
         rdfs:label "Setback outcome" ;
         rdfs:seeAlso pzo2101:checkCriticalFailure .


###  https://raw.githubusercontent.com/AbsVahter/trpgontologies/main/gumshoe/cthulhu_confidential/PELGOC01.ttl#uses
:uses rdf:type owl:ObjectProperty ;
      rdfs:domain :Gamerule ;
      rdfs:range owl:Thing ;
      rdfs:label "uses" ;
      rdfs:seeAlso pzo2101:uses .


