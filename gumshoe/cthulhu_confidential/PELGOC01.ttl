###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/gumshoe/cthulhu_confidential/PELGOC01.ttl#Test
:Test rdf:type owl:Class ;
      rdfs:subClassOf bc:Gamerule ;
      sche:description """{threshold: [probability by dice number - 1 dice, 2 dices, 3 dices]} = {
    2: [83%, 100%, 100%] ;
    3: [67%, 97%, 100%] ;
    4: [50%, 92%, 99.5%] ;
    5: [33%, 83%, 98%] ;
    6: [17%, 72%, 95%] ;
    7: [-, 58%, 91%] ;
    8: [-, 42%, 84%] ;
    9: [-, 28%, 74%] ;
    10: [-, 17%, 62.5%] ;
    11: [-, 8%, 50%] ;
    12: [-, 3%, 37.5%] ;
    13: [-, -, 26%] ;
    14: [-, -, 16%] ;
    15: [-, -, 9%] ;
    16: [-, -, 5%] ;
    17: [-, -, 2%] ;
