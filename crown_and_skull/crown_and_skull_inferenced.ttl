###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/crown_and_skull/crown_and_skull.ttl#wild_magic
:wild_magic rdf:type owl:NamedIndividual ,
                     gen:Act ,
                     gen:Check ;
            gen:Actor gen:player ;
            gen:basedOn :check_skill ;
            gen:checkFailure :losing_control ;
            gen:checkSuccess :set_spell ;
            gen:checkThreshold :threshold_wild_magic ;
            gen:effect :losing_control ,
                       :set_spell ;
            gen:input :spell_custom ,
                      :threshold_wild_magic ;
            gen:prerequisite :skill_magic ;
            gen:duration "1 turn" ;
            gen:prerequisiteDescription "magic skill > spell value in hero points" ;
            gen:valueFormula """
{
    d20 = 1: critical success ;
    d20 <= threshold: success ;
    d20 > threshold: failure ;
    d20 = 20: critical failure ;
}
""" ;
            rdfs:label "Conjuring a Wild Spell"@en .


