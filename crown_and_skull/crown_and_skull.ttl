###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/crown_and_skull/crown_and_skull.ttl#creation_custom_companion_base
:creation_custom_companion_base rdf:type owl:NamedIndividual ;
                                gen:inputOption :Companion ;
                                gen:set :companion_custom ;
                                gen:valueFormula """
cost = (base companion cost) or
{
    small animal (d4 attacks, 1 skill value 9, 6 defense): 3 points ;
    medium sized beasts or simple devices (d6 attacks, 2 skills value 9, 9 defense): 5 points ;
    large animals or retainers (d8 attacks, 3 skills value 9, 9 defense): 12 points ;
}
""" ;
                                rdfs:label "Custom companion: base"@en .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/crown_and_skull/crown_and_skull.ttl#creation_custom_companion_skills
:creation_custom_companion_skills rdf:type owl:NamedIndividual ;
                                  gen:after :creation_custom_companion_base ;
                                  gen:setAdditive :companion_custom ;
                                  gen:valueObject :SkillCompanion ;
                                  gen:prerequisiteDescription "number of skills <= maximum of skills of base" ;
                                  rdfs:label "Custom companion: skills"@en .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/crown_and_skull/crown_and_skull.ttl#creation_custom_companion_upgrade
:creation_custom_companion_upgrade rdf:type owl:NamedIndividual ;
                                   gen:after :creation_custom_companion_skills ;
                                   gen:resource :hero_points ;
                                   gen:setAdditive :companion_custom ;
                                   gen:valueObject :EffectCompanion ,
                                                   :LimitationCompanion ;
                                   gen:prerequisiteDescription "final cost > 0" ;
                                   gen:valueFormula """
cost += {
    effect: 3 points ;
    limitation: -3 points ;
}
""" ;
                                   rdfs:label "Custom companion: upgrade"@en .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/crown_and_skull/crown_and_skull.ttl#creation_custom_item
