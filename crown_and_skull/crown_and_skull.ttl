###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/ontoGeneral.ttl#Check
gen:Check rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty gen:Actor ;
                            owl:hasValue gen:player
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty gen:valueFormula ;
                            owl:hasValue """
{
    d20 = 1: critical success ;
    d20 <= threshold: success ;
    d20 > threshold: failure ;
    d20 = 20: critical failure ;
}
"""
                          ] .


