                               rdfs:label "Custom spell: upgrade"@en ;
                               gen:after :creation_custom_spell_base ;
                               gen:prerequisiteDescription "(no stacking limitations) and (effect's stack < 4) and (final cost > 0)" ;
                               gen:resource :hero_points ;
                               gen:setAdditive :spell_custom ;
                               gen:valueFormula """
cost += {
    effect: 3 points ;
    limitation: -3 points ;
}
""" ;
                               gen:valueObject :EffectSpell ,
                                               :LimitationSpell .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/crown_and_skull/crown_and_skull.ttl#crown
:crown rdf:type owl:NamedIndividual ;
       rdfs:comment "Those who choose the CROWN will join an order of protectors, knights, and heralds sworn to the well-being of the realm, the sanctity of all life, and the upholding of ancient codes. They swear to honor the decrees of kings and queens, to serve as a beacon of truth and loyalty." ;
       rdfs:label "Crown"@en ;
       gen:included :crown_gain_ring ,
                    :crown_gaining_hero_points ,
                    :crown_select_equipment ,
                    :crown_select_skill ,
                    :crown_set_flaw .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/crown_and_skull/crown_and_skull.ttl#crown_gain_ring
