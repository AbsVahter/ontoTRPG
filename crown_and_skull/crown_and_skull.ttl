                      owl:equivalentClass [ rdf:type owl:Restriction ;
                                            owl:onProperty gen:setAdditive ;
                                            owl:hasValue :equipment
                                          ] ;
                      rdfs:subClassOf gen:Act ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty gen:prerequisiteDescription ;
                                        owl:hasValue "final number of equipment <= 10"
                                      ] .


