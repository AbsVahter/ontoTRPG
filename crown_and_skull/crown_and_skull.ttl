###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/crown_and_skull/crown_and_skull.ttl#creation_custom_companion
:creation_custom_companion rdf:type owl:NamedIndividual ;
                           gen:Actor gen:player ;
                           gen:included :creation_custom_companion_base ,
                                        :creation_custom_companion_skills ,
                                        :creation_custom_companion_upgrade ;
                           gen:set :companion_custom ;
