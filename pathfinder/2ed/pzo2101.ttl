:sorcerer rdf:type owl:NamedIndividual ,
                   :Gameclass ;
          :abilityBoost :charisma ;
          :experted :will ;
          :selects :advanced_bloodline ,
                   :arcane_evolution ,
                   :bespell_weapon ,
                   :bloodline_conduit ,
