<owl:NamedIndividual rdf:about="#damage_check">
  <rdf:type rdf:resource="#Gamerule"/>
  <changes rdf:resource="#hit_points"/>
  <uses rdf:resource="#immunities"/>
  <uses rdf:resource="#weaknesses_and_resistances"/>
  <leadsTo rdf:resource="#zero_hp_listener"/>
  <uses rdf:resource="#hit_points"/>
  <leadsTo rdf:resource="#deactivate_unconscious"/>
  <leadsTo rdf:resource="#massive_damage"/>
  <deactivates rdf:resource="#temporary_hit_points"/>
  <activates rdf:resource="#prone"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Damage check</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In the midst of combat, you attempt checks to determine if you can damage your foe with weapons, spells, or  alchemical concoctions. On a successful check, you hit  and deal damage. Damage decreases a creature’s Hit  Points on a 1-to-1 basis.</rdfs:comment>
  <calculations rdf:datatype="http://www.w3.org/2001/XMLSchema#string">damage intermediate = ((damage roll) + Max(weakness) + Min(resistance)) * (0 if immunity to damage type)</calculations>
  <calculations rdf:datatype="http://www.w3.org/2001/XMLSchema#string">damage = temporary hit points - damage intermediate</calculations>
  <calculations rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Hit points change = - damage</calculations>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#damage_roll">
