  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Adventurers who don’t wear armor travel in durable clothing.</rdfs:comment>
  <hardness rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</hardness>
  <price rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1 sp</price>
