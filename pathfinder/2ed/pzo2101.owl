          <owl:hasValue rdf:resource="#reflex"/>
        </owl:Restriction>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#trained"/>
          <owl:hasValue rdf:resource="#will"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#trained"/>
      <owl:hasValue rdf:resource="#fist"/>
    </owl:Restriction>
  </rdfs:subClassOf>
