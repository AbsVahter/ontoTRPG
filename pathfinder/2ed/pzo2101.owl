<owl:NamedIndividual rdf:about="#art_aspis_consortium_icon">
  <rdf:type rdf:resource="#Icon"/>
  <depicts rdf:resource="#aspis_consortium"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aspis Consortium</rdfs:label>
  <sche:image rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://beadleandgrimms.com/cdn/shop/products/AspisConsortiumtoken.png?v=1654797266&amp;width=823</sche:image>
