<owl:Class rdf:about="#CheckRoll">
  <rdfs:subClassOf rdf:resource="#Gamerule"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#calculations"/>
      <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">roll = d20 + ability modifier + proficiency bonus + circumstance modifier + status modifier + item modifier + untyped penalties</owl:hasValue>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#calculations"/>
