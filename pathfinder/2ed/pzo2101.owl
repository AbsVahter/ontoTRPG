</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#human">
  <rdf:type rdf:resource="#Playable_ancestry"/>
  <rdf:type>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#has_selectable_feat"/>
      <owl:someValuesFrom rdf:resource="#Language_common"/>
    </owl:Restriction>
