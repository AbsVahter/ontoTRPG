<owl:NamedIndividual rdf:about="#range_penalty">
  <rdf:type rdf:resource="#Gamerule"/>
  <uses rdf:resource="#equipment"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ranged and thrown weapons each have a listed range increment, and attacks with them grow less accurate against targets farther away.</rdfs:comment>
