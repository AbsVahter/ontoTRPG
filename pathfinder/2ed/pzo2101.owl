<owl:Class rdf:about="#Gear_and_services">
  <rdfs:subClassOf rdf:resource="#Equipment"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Your character needs all sorts of items both while exploring and in downtime, ranging from rations to climbing gear to fancy clothing, depending on the situation.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Equipment_pack">
  <rdfs:subClassOf rdf:resource="#Equipment"/>
