<owl:NamedIndividual rdf:about="#multiple_attack_penalty">
  <rdf:type rdf:resource="#Gamerule"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The more attacks you make beyond your first in a single turn, the less accurate you become.</rdfs:comment>
  <calculations rdf:datatype="http://www.w3.org/2001/XMLSchema#string">penalty = Max((i - 1) * (-5), -10), for i-th attack in turn</calculations>
