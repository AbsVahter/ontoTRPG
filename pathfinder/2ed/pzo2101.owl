<owl:Class rdf:about="#Gameclass">
  <rdfs:subClassOf rdf:resource="#Concept"/>
  <rdfs:subClassOf>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <owl:Restriction>
          <owl:onProperty rdf:resource="#trained"/>
          <owl:hasValue rdf:resource="#perception"/>
        </owl:Restriction>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#trained"/>
          <owl:hasValue rdf:resource="#fortitude"/>
        </owl:Restriction>
