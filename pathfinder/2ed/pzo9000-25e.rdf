    <!-- https://raw.githubusercontent.com/AbsVahter/trpgontologies/main/pathfinder/2ed/pzo9000-25e.rdf#Event_Free_Kintargo -->

    <owl:NamedIndividual rdf:about="https://raw.githubusercontent.com/AbsVahter/trpgontologies/main/pathfinder/2ed/pzo9000-25e.rdf#Event_Free_Kintargo">
