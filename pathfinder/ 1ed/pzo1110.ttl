###  https://github.com/AbsVahter/trpgontologies/pathfinder/1ed/pzo1110#AbilModRules
pzo1110:AbilModRules rdf:type owl:Class ;
                     rdfs:subClassOf pzo1110:Rules ,
                                     [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                              owl:onProperty pzo1110:useRule ;
                                                              owl:someValuesFrom pzo1110:AbilityScores
                                                            ]
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty pzo1110:output ;
                                                              owl:hasValue "floor((score - 10)/2)"
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                     rdfs:comment "Each ability, after changes made because of race, has a modifier ranging from –5 to +5." .


###  https://github.com/AbsVahter/trpgontologies/pathfinder/1ed/pzo1110#AbilityRoll
pzo1110:AbilityRoll rdf:type owl:Class ;
                    rdfs:subClassOf pzo1110:Rolls ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty pzo1110:useRule ;
                                      owl:someValuesFrom pzo1110:AbilModRules
                                    ] .


###  https://github.com/AbsVahter/trpgontologies/pathfinder/1ed/pzo1110#AbilityScores
pzo1110:AbilityScores rdf:type owl:Class ;
                      rdfs:subClassOf pzo1110:CharFeatures ,
                                      [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                               owl:onProperty pzo1110:isPickedBy ;
                                                               owl:hasValue pzo1110:AbilitySelector
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty pzo1110:isSetByRule ;
                                                               owl:hasValue pzo1110:AbilityScoresSetter
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty pzo1110:isSetByRule ;
                                                               owl:hasValue pzo1110:LvlUpAbilityScoreSetter
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty pzo1110:isSetByRule ;
                                                               owl:hasValue pzo1110:VariedNatureAbilityScoreModifiersRule
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                      rdfs:comment "Each character has six ability scores that represent his character’s most basic attributes. They are his raw talent and prowess. While a character rarely rolls an ability check (using just an ability score), these scores, and the modifiers they create, affect nearly every aspect of a character’s skills and abilities. Each ability score generally ranges from 3 to 18, although racial bonuses and penalties can alter this; an average ability score is 10." ;
                      rdfs:label "Ability scores" .


