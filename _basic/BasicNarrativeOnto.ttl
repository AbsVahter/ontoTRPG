@prefix : <https://raw.githubusercontent.com/AbsVahter/trpgontologies/main/_basic/BasicNarrativeOnto.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sche: <http://schema.org/> .
@base <https://raw.githubusercontent.com/AbsVahter/trpgontologies/main/_basic/BasicNarrativeOnto.ttl#> .

<https://raw.githubusercontent.com/AbsVahter/trpgontologies/main/_basic/BasicNarrativeOnto.ttl#> rdf:type owl:Ontology .

:Narrative rdf:type owl:Class ;
         rdfs:subClassOf owl:Thing .

:Agent rdf:type owl:Class ;
       rdfs:subClassOf :Narrative ;
       rdfs:label "Agent" .

:Character rdf:type owl:Class ;
           rdfs:subClassOf :Agent ;
    rdfs:label "Character" .

:Location rdf:type owl:Class ;
          rdfs:subClassOf :Narrative ;
          rdfs:label "Location" .

:location rdf:type owl:ObjectProperty ;
          rdfs:domain :Narrative ;
          rdfs:range :Location ;
    rdfs:label "location" .