###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#Weapon
:Weapon rdf:type owl:Class ;
        rdfs:subClassOf :Equipment ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :model ;
                          owl:hasValue :weapon
                        ] .


