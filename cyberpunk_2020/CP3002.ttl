#################################################################

###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#action
:action rdf:type owl:ObjectProperty ;
        rdfs:domain :Act ;
        rdfs:range :Action .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#actor
:actor rdf:type owl:ObjectProperty ;
       rdfs:domain :Act ;
       rdfs:range :Actor .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#after
:after rdf:type owl:ObjectProperty ;
       rdfs:domain :Act ;
       rdfs:range :Act .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#career_skill
:career_skill rdf:type owl:ObjectProperty ;
              rdfs:domain :Role ;
              rdfs:range :Skill .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#includes
:includes rdf:type owl:ObjectProperty .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#input
:input rdf:type owl:ObjectProperty ;
       rdfs:domain :Act ;
       owl:propertyChainAxiom ( :after
                                :result
                              ) .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#model
:model rdf:type owl:ObjectProperty .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#option
:option rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :after ;
        sche:description "xor condition by default" .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#resource
:resource rdf:type owl:ObjectProperty ;
          rdfs:domain :Act ;
          sche:description "increase or decrease volume of resource" .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#result
:result rdf:type owl:ObjectProperty ;
        rdfs:domain :Act .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#specialAbility
:specialAbility rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :career_skill ;
                rdfs:domain :Role ;
                rdfs:range :SpecialAbility .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#statistic
:statistic rdf:type owl:ObjectProperty ;
           rdfs:domain :Skill ;
           rdfs:range :Statistic .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#subAct
:subAct rdf:type owl:ObjectProperty ;
        rdfs:domain :Act ;
        rdfs:range :Act .


#################################################################
#    Data properties
#################################################################

###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#accuracy
:accuracy rdf:type owl:DatatypeProperty ;
          rdfs:domain :Weapon ;
          rdfs:range xsd:integer .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#armorCover
:armorCover rdf:type owl:DatatypeProperty ;
            rdfs:domain :Armor ;
            rdfs:range xsd:string .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#availability
:availability rdf:type owl:DatatypeProperty ;
              rdfs:domain :Weapon ;
              rdfs:range xsd:integer ;
              sche:description """
{
    1: excellent ;
    2: common ;
    3: poor ;
    4: rare ;
}
              """ .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#concealability
:concealability rdf:type owl:DatatypeProperty ;
                rdfs:domain :Weapon ;
                rdfs:range xsd:integer ;
                sche:description """
{
    1: pocket, pants leg, sleeve ;
    2: jacket, coat, shoulder rig ;
    3: long coat ;
    4: can't be hidden ;
}""" .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#condition
:condition rdf:type owl:DatatypeProperty ;
           rdfs:domain :Act .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#cost
:cost rdf:type owl:DatatypeProperty ;
      rdfs:domain :Equipment ;
      rdfs:range xsd:integer .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#damage
:damage rdf:type owl:DatatypeProperty ;
        rdfs:domain :Weapon ;
        rdfs:range xsd:string .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#difficulty_modifier_of_skill
:difficulty_modifier_of_skill rdf:type owl:DatatypeProperty ;
                              rdfs:domain :Skill ;
                              rdfs:range xsd:integer ;
                              sche:description "1 by default" .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#encumberanceValue
:encumberanceValue rdf:type owl:DatatypeProperty ;
