#################################################################
#    Classes
#################################################################

###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#Act
:Act rdf:type owl:Class .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#Action
:Action rdf:type owl:Class .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#Actor
:Actor rdf:type owl:Class .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#Armor
:Armor rdf:type owl:Class ;
       rdfs:subClassOf :Equipment .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#Equipment
:Equipment rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :model ;
                             owl:hasValue :equipment
                           ] .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#ExoticWeapon
:ExoticWeapon rdf:type owl:Class ;
              rdfs:subClassOf :Weapon .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#HeavyWeapon
:HeavyWeapon rdf:type owl:Class ;
             rdfs:subClassOf :Weapon ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :rate_of_fire ;
                               owl:hasValue 1
                             ] .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#MartialArts
:MartialArts rdf:type owl:Class ;
             rdfs:subClassOf :Skill ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :statistic ;
                               owl:hasValue :reflex
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty [ owl:inverseOf :career_skill
                                              ] ;
                               owl:hasValue :solo
                             ] .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#MeleeWeapon
:MeleeWeapon rdf:type owl:Class ;
             rdfs:subClassOf :Weapon ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :reliability ;
                               owl:hasValue 1
                             ] .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#Pistol
:Pistol rdf:type owl:Class ;
        rdfs:subClassOf :Weapon ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :range ;
                          owl:hasValue 50
                        ] .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#Rifle
:Rifle rdf:type owl:Class ;
       rdfs:subClassOf :Weapon ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :concealability ;
                         owl:hasValue 4
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :range ;
                         owl:hasValue 400
                       ] .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#Role
:Role rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :model ;
                        owl:hasValue :role
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :specialAbility ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :SpecialAbility
                      ] .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#Shotgun
:Shotgun rdf:type owl:Class ;
         rdfs:subClassOf :Weapon ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :concealability ;
                           owl:hasValue 4
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :damage ;
                           owl:hasValue "4d6 (00)"
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :range ;
                           owl:hasValue 50
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :reliability ;
                           owl:hasValue 2
                         ] .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#Skill
:Skill rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :model ;
                         owl:hasValue :skills
                       ] .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#SpecialAbility
:SpecialAbility rdf:type owl:Class ;
                rdfs:subClassOf :Skill .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/cyberpunk_2020/CP3002.ttl#Statistic
:Statistic rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :model ;
                             owl:hasValue :stats
