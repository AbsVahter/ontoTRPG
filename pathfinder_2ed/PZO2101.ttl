###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/pathfinder_2ed/PZO2101.ttl#goblin
:goblin rdf:type owl:NamedIndividual ,
                 :AncestryForPlayer ;
        :Feat :burn_it ,
              :cave_climber ,
              :city_scavenger ,
              :expertise_weapon_goblin ,
              :goblin_lore ,
              :goblin_scuttle ,
              :goblin_song ,
              :junk_tinker ,
              :rough_rider ,
              :skittering_scuttle ,
              :very_sneaky ,
              :very_very_sneaky ,
              :weapon_familiarity_goblin ,
              :weapon_familiarity_goblin_adv ;
        :Heritage :goblin_charhide ,
                  :goblin_irongut ,
                  :goblin_razortooth ,
                  :goblin_snow ,
                  :goblin_unbreakable ;
        :Language :lang_goblin ;
        :SpecialAbility :darkvision ;
        :abilityBoost :ability_score_charisma ,
                      :ability_score_dexterity ;
        :abilityFlaw :ability_score_wisdom ;
        :languageAdditional :lang_draconic ,
                            :lang_dwarven ,
                            :lang_gnoll ,
                            :lang_gnomish ,
                            :lang_halfling ,
                            :lang_orcish ;
        :abilityBoostFree 1 ;
        :hp 6 ;
        :size -1 ;
        :speed 25 ;
        rdfs:comment "Goblins are a short, scrappy, energetic people who have spent millenia maligned and feared."@en ,
                     "The convoluted histories other people cling to don’t interest goblins. These small folk live in the moment, and they prefer tall tales over factual records. The wars of a few decades ago might as well be from the ancient past. Misunderstood by other people, goblins are happy how they are. Goblin virtues are about being present, creative, and honest. They strive to lead fulfilled lives, rather than worrying about how their journeys will end. To tell stories, not nitpick the facts. To be small, but dream big."@en ,
                     "Гоблины - низкорослый, проворный и энергичный народец, который тысячелетиями недолюбливали и боялись."@ru ,
                     "Другие народы хранят в памяти свое пыльное и путаное историческое наследие, но гоблинов подобное совершенно не интересует. Они живут одним днем и считают похвальбу и небылицы куда увлекательнее летописей. Война десятилетней давности для них - дела давно минувших дней. Гоблинов часто не понимают, но они живут собственной счастливой жизнью. Для них важнее всего настоящее, изобретательность и откровенность. Они стремятся жить на полную катушку, а не беспокоиться, как закончится их земной путь. Рассказывать истории, а не перебирать факты. Быть пусть и маленьким, зато с большим воображением."@ru ;
        rdfs:label "Goblin"@en ,
                   "Гоблин"@ru .


