###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/pathfinder_2ed/PZO2101.ttl#adapted_cantrip
:adapted_cantrip rdf:type owl:NamedIndividual ;
                 gen:effect :adapted_cantrip_replacing_cantrip ,
                         :adapted_cantrip_selection_of_cantrip ;
                 :required_min_level 1 ;
                 rdfs:comment "Through study of multiple magical traditions, you've altered a spell to suit your spellcasting style"@en ,
                              "Вы изучили несколько магических традиций и адаптировали одно из заклинаний под свою собственную"@ru ;
                 rdfs:label "Adapted cantrip"@en ,
                            "Заимствованный фокус"@ru ;
                 gen:prerequisite :Spell .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/pathfinder_2ed/PZO2101.ttl#adapted_cantrip_replacing_cantrip
:adapted_cantrip_replacing_cantrip rdf:type owl:NamedIndividual ;
    gen:modified :SpellCantrip ;
                                   gen:methodDescription "class cantrip -> selected cantrip" ;
                                   rdfs:label "Adapted cantrip: replacing class cantrip by selected cantrip"@en ,
                                              "Заимствованный фокус: замена классового фокуса на выбранный"@ru ;
                                   gen:after :adapted_cantrip_selection_of_cantrip .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/pathfinder_2ed/PZO2101.ttl#adapted_cantrip_selection_of_cantrip
:adapted_cantrip_selection_of_cantrip rdf:type owl:NamedIndividual ;
                                      :selected :SpellCantrip ,
                                                :adapted_tradition ;
                                      gen:methodDescription """
cantrips += selected cantrip ;
tradition of selected cantrip = human's magic tradition ;
""" ;
                                      rdfs:label "Adapted cantrip: selection of cantrip"@en ,
                                                 "Заимствованный фокус: выбор фокуса"@ru ;
                                      gen:input :MagicalTradition ;
                                      gen:prerequisiteDescription "cantrip tradition != human's magic tradition" .


