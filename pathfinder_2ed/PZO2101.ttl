###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/pathfinder_2ed/PZO2101.ttl#Leveling_up
:Leveling_up rdf:type owl:NamedIndividual ;
             :gained :Level ,
                       :hp_max ,
                       :xp;
             gen:actor :Character ;
             gen:methodDescription """
level += 1 ;
xp -= 1000 ;
maximum hp += class hp + constitution modifier ;
""" ;
             rdfs:label "Leveling up"@en ,
                        "Повышение в уровне"@ru ;
             gen:after :pc_gained_xp ;
             gen:input :GameClass ,
                      :ability_score_constitution ,
                      :pc ;
             gen:prerequisiteDescription "(level < 20) and (xp >= 1000)" .


