###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/pathfinder_2ed/PZO2101.ttl#creation_of_ability_scores_alt
:creation_of_ability_scores_alt rdf:type owl:NamedIndividual ;
                                gen:actor gen:player ;
gen:set :AbilityScore ;
                                gen:methodDescription """
flaw = -2 ;
boost = +2 ;
ancestry boosts and flaws count = ((2 boosts + 1 flaw) xor (1 free boosts if human or alternative ancestry method)) + (voluntary flaw) ;
background boosts count = (1 boost from two options) ;
ability score = (4d6 - min) + (ancestry boost + ancestry flaw) + (background boost) ;
""" ;
                                rdfs:label "Player determined ability scores. Alternative method"@en ,
                                           "Игрок определяет значения характеристик. Альтернативный метод"@ru ;
                                gen:after :selection_of_ancestry ,
                                         :selection_of_background ;
                                gen:input :abilityBoost ,
                                         :abilityBoostFree ,
                                         :abilityFlaw ;
                                gen:prerequisiteDescription "(ability score <= 18) and (different abilities from one source of boost or flaws)" .


###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/pathfinder_2ed/PZO2101.ttl#creation_of_ability_scores_standard
:creation_of_ability_scores_standard rdf:type owl:NamedIndividual ;

                                     gen:actor gen:player ;
gen:set :AbilityScore ;
                                     gen:methodDescription """
flaw = -2 ;
boost = +2 ;
ancestry boosts and flaws count = ((2 boosts + 1 free boost + 1 flaw) xor (2 free boosts if human or alternative ancestry method)) + (voluntary flaw) ;
background boosts count = (1 boost from two options) + (1 free boost) ;
class boosts count = 1 ;
free boosts count = 4 ;
ability score = 10 + (ancestry boost and ancestry flaw and ancestry free boost) + (background boost) + (class boost) + (free boost) ;
""" ;
                                     rdfs:label "Player determined ability scores. Standard method"@en ,
                                                "Игрок определяет значения характеристик. Стандартный метод"@ru ;
                                     gen:after :selection_of_ancestry ,
                                              :selection_of_background ,
                                              :selection_of_class ;
                                     gen:input :abilityBoost ,
                                              :abilityBoostFree ,
                                              :abilityFlaw ;
                                     gen:prerequisiteDescription "(ability score >= 8) and (ability score <= 18) and (different abilities from one source of boost or flaws)" .


