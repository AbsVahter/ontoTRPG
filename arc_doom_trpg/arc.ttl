###  https://raw.githubusercontent.com/AbsVahter/ontoTRPG/main/arc_doom_trpg/arc.ttl#check_skill_result_upgrade
:check_skill_result_upgrade rdf:type owl:NamedIndividual ;
                            gen:after :check_skill ;
                            gen:modified :check_skill_fail ,
                                         :check_skill_fail_with_opportunity ,
                                         :check_skill_success_with_consequence ;
                            gen:resource :blood ,
                                         :guts ;
                            gen:valueObject :check_skill_fail_with_opportunity ,
                                            :check_skill_success ,
                                            :check_skill_success_with_consequence ;
                            gen:cost "3" ;
                            gen:valueFormula """
(blood -= cost) or (guts -= cost) ;
fail with opportunity -> success ;
success with consequence -> success ;
fail -> (fail with opportunity) xor (success with consequence) ;
""" ;
                            rdfs:label "Upgrade skill check result"@en .


